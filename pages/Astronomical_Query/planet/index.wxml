  <view class='text-content'>


    <view class="table">
      <view class="tr">
        <view class="th">属性</view>
        <view class="th">值</view>
     

      </view>
      <view class="tr">
        <view class="td">偏心率</view>
        <view class="td" style='font-size:12px;color:#3763BA ;font-weight:bolder '>{{p.Bias_rate}}</view>
       


      </view>
      <view class="tr">
        <view class="td">赤道重力</view>
        <view class="td" style='font-size:12px;color:#3763BA ;font-weight:bolder '>{{p.Equatorial_gravity}}</view>
      

      </view>
      <view class="tr">
        <view class="td">赤道倾角</view>
        <view class="td" style='font-size:12px;color:#3763BA ;font-weight:bolder '>{{p.Equatorial_inclination}}</view>
       


      </view>
      <view class="tr">
        <view class="td">赤道半径</view>
        <view class="td" style='font-size:12px;color:#3763BA ;font-weight:bolder '>{{p.Equatorial_radius}}</view>
        
      </view>
      <view class="tr">
        <view class="td">卫星数量</view>
        <view class="td" style='font-size:12px;color:#3763BA ;font-weight:bolder '>{{p.Number_of_satellites}}</view>
      
      </view>
      <view class="tr">
        <view class="td">轨道半径</view>
        <view class="td" style='font-size:12px;color:#3763BA ;font-weight:bolder '>{{p.Orbital_Radius}}</view>
     

      </view>
      <view class="tr">
        <view class="td">轨道倾角</view>
        <view class="td" style='font-size:12px;color:#3763BA ;font-weight:bolder '>{{p.Orbital_inclination}}</view>
       


      </view>
      <view class="tr">
        <view class="td">比重</view>
        <view class="td" style='font-size:12px;color:#3763BA ;font-weight:bolder '>{{p.Proportion}}</view>
       
      </view>
      <view class="tr">
        <view class="td">质量（地球为1）</view>
        <view class="td" style='font-size:12px;color:#3763BA ;font-weight:bolder '>{{p.Quality}}</view>
       
      </view>
      <view class="tr">
        <view class="td">自转周期</view>
        <view class="td" style='font-size:12px;color:#3763BA ;font-weight:bolder '>{{p.Rotation_cycle}}</view>
    
      </view>
      <view class="tr">
        <view class="td">公转周期</view>
        <view class="td" style='font-size:12px;color:#3763BA ;font-weight:bolder '>{{p.Transit_cycle}}</view>
    
      </view>

     <view class="tr">
        <view class="td">体积(地球为1)</view>
        <view class="td" style='font-size:12px;color:#3763BA ;font-weight:bolder '>{{p.Volume}}</view>
    
      </view>
    </view>
 


  </view>
